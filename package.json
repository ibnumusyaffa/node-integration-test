{
  "name": "starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./app/server.js",
    "test": "NODE_ENV=test mocha --require test/setup.js --recursive",
    "dev": "nodemon ./app/server.js",
    "lint": "eslint --ignore-path .gitignore  --ext .js,.ts,.tsx .",
    "lint:fix": "eslint --ignore-path .gitignore  --ext .js,.ts,.tsx --fix .",
    "format": "pretty-quick --ignore-path .gitignore --pattern \"**/*.+(js|jsx|tsx)\"",
    "format:all": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|tsx)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|jsx|tsx)\"",
    "validate": "npm run lint && npm run format && npm run test",
    "migrate:make": "npx knex migrate:make",
    "migrate": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "seed:make": "npx knex seed:make",
    "seed": "npx knex seed:run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.2.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "@faker-js/faker": "^7.6.0",
    "@jackfranklin/test-data-bot": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.3.3"
  }
}
